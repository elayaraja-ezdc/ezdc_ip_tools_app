
<div style="width:700px; margin: 50px auto;" class="border p-3">
  <h3>Executing......</h3>

  <div class="progress">
    <div class="progress-bar progress-bar-striped active" role="progressbar" id="progressbar"
  aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width:100%">
      40%
    </div>  
  </div>
</div>

<script>
    var i = 100;
    var counterBack = setInterval(function(){
    i--;
    if(i>=-1){
        document.getElementById("progressbar").style.width = i+1+"%";
        document.getElementById("progressbar").innerHTML = i+1+"%";
        if(i == 0) {
            window.location.href = "<%= url_for('http://'+request.host+':3000/workbook/console')%>"
        }
    } else {
    clearTimeout(counterBack);
    }

    }, 100);

</script>
